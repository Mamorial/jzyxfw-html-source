.shouye{
	width:100%;
	height:100%;
	border: 1px solid #ccc;
	position: relative;
	float: none;
	text-align: center;
	margin: 0px 0 0 0;
	background:#fff;
	padding-bottom: 50px;
}
.shouye .head_left{
	float: left;
	width:100%;
	height:150px;
	border: 0px solid red;
	float: left;
	
	position: relative;
}

.shouye .head_center1{
	width:50%;
	height:150px;
	
	float:left;
	
	
}
.shouye .head_right{
	width: 100%;
	height: 150px;
	border:1px solid #ddd;
	float: left;
	text-align: center;
	position: relative;
	
	border-radius: 5px;
	box-shadow: 2px 2px 2px 2px #ddd;
}
