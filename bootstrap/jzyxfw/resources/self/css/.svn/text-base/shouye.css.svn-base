.shouye .center{
	
	
	border: 0px solid red;
	position: relative;
	
}
.shouye .center .center_left{
	width: 100%;
	height: 100%;
	float:left;
	border: 1px solid #ddd;
	margin-right:1%;
	border-radius: 5px;
	box-shadow: 2px 2px 2px 2px #ddd;
}
.shouye .center .center_center{
	width: 100%;
	height: 100%;
	float:left;
	border: 1px solid #ddd;
	margin-right:1%;
	border-radius: 5px;
	box-shadow: 2px 2px 2px 2px #ddd;
}
.shouye .center .center_right{
	width: 100%;
	height: 100%;
	float:left;
	border: 1px solid #ddd;
	border-radius: 5px;
	box-shadow: 2px 2px 2px 2px #ddd;
}
.shouye .bottom{
	width: 93%;
	height: 200px;
	border: 0px solid red;
	position: relative;
	margin-top: 0px;
	margin-left:3%;
	padding:5px;
}
.shouye .bottom_left{
	width: 100%;
	height: 150px;
	border:0px solid #95B8E7;
	float: left;
	text-align: center;
	position: relative;
}

.shouye .bottom_left #tt table{
	width:100%;
	height:100px;
	border:none;
}
.shouye .bottom_left #tt table tr td{
	text-align:center;
	border:1px solid #ddd;
	font-size:12px;
}

